#: basewindow.rstrstop
msgid "STOP"
msgstr "BEIGT"

#: help.hkey01
msgid "Be patient!"
msgstr "Esi pacietīgs!"

#: help.hkey03
msgid "Any interactive changes are carried out within 1 second interval."
msgstr "Programma atsaucas uz izmaiņām tikai reizi sekundē."

#: help.hkey05
msgid "[Space], [s], Mouse click - start/stop the timer."
msgstr "[Atstarpe], [s], peles klikšķis – Sākt/apstādināt laika atskaiti."

#: help.hkey06
msgid "[F11], [f], Mouse double-click  - Full screen on/off."
msgstr "[F11], [f], peles dubultklikšķis – Ieslēgt/izslēgt pilnekrāna skatu."

#: help.hkey07
msgid "[Esc] - Exit."
msgstr "[Esc] – Iziet."

#: help.hkey08
msgid "[b] - Border-less on/off."
msgstr "[b] – Noņemt/atjaunot loga apmales."

#: help.hkey09
msgid "[F1], [h] - This help."
msgstr "[F1], [h] – Šis paskaidrojums."

#: help.hkey10
msgid "[m], Mouse right click - Settings window."
msgstr "[m], peles „labā” poga – Pulksteņa iestatījumi."

#: help.hkey11
msgid "[Left], [Up] - Increase time by 1 minute."
msgstr "[Pa_kreisi], [Uz_augšu] – Palielināt laiku par 1 minūti."

#: help.hkey12
msgid "[Right], [Down] - Decrease time by 1 minute."
msgstr "[Pa_labi], [Uz_leju] – Samazināt laiku par 1 minūti."

#: help.hkey13
msgid "[r], Mouse middle click - reset timer."
msgstr "[r], Peles vidējā poga – palaists laika atskaiti no jauna."

#: help.hkey14
msgid "You can change current time with the mouse wheel as well."
msgstr "Patreizējo laiku iespējams mainīt ar peles ritenīti."

